        "mongodb-core": "^3.2.7",
        "mongoose": "^8.7.0",
        "multer": "^2.0.2",
        "multer-s3": "^3.0.1",
        "node-email-verifier": "^3.4.1",
        "node-schedule": "^2.1.0",
        "nodemailer": "^7.0.7",
        "prettier": "^3.0.3",
        "python-shell": "^5.0.0",
        "redis": "^5.8.3",
        "serve-favicon": "^2.5.0",
        "winston": "^3.8.2"
    },
